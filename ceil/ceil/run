#! /usr/bin/env bash

(time smack --float --unroll 1 --integer-encoding bit-vector --verifier corral --entry-points musl_smack -- ceil.rs ceil.c) > cms.txt 2>&1 &
(time smack --float --unroll 1 --integer-encoding bit-vector --verifier corral --entry-points rust_smack -- ceil.rs ceil.c) > crs.txt 2>&1 &
(time smack --float --unroll 1 --integer-encoding bit-vector --verifier corral --entry-points musl_rust -- ceil.rs ceil.c) > cmr.txt 2>&1 &
(time smack --float --unroll 1 --integer-encoding bit-vector --verifier boogie --entry-points musl_smack -- ceil.rs ceil.c) > bms.txt 2>&1 &
(time smack --float --unroll 1 --integer-encoding bit-vector --verifier boogie --entry-points rust_smack -- ceil.rs ceil.c) > brs.txt 2>&1 &
(time smack --float --unroll 1 --integer-encoding bit-vector --verifier boogie --entry-points musl_rust -- ceil.rs ceil.c) > bmr.txt 2>&1 &

